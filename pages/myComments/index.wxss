page{
  background-color: #f5f5f5;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
empty-layout{
  flex: 1;
}
.i_pjlb {
  display: flex;
  /* padding: 30rpx; */
  flex-direction: column;
}

.i_pjlb .i_pjlb_item {
  width: 100%;
}

.i_pjlb .i_pjlb_item_head {
  width: 100%;
  display: flex;
}

.i_pjlb_item_head .i_pjlb_item_head_content {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  margin-top: 20rpx;
}

.i_pjlb_item_text {
  margin-left: 80rpx;
  margin-top: 10rpx;
  margin-right: 30rpx;
}

.i_pjlb_item_img {
  margin-left: 80rpx;
  margin-right: 30rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.i_pjlb_item_img .img_item {
  width: 130rpx;
  height: 130rpx;
  margin: 10rpx;
  margin-bottom: 20rpx;
}
.rate {
  width: 180rpx;
  height: 25rpx;
}