/* pages/yjfk/index.wxss */
.container{
  width: 100%;
  height: 100%;
}
.background{
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.bg_center{
  width: 703rpx;
  height: 1087rpx;
  position:fixed;
  top: 50%;
  left: 50%; 
  transform: translate(-50%,-50%);
  z-index: -1;
}
.bg_center>image{
  width: 100%;
  height: 100%;
  position:fixed;
  left: 0;
  top: 0;
  background-size: 100% 100%;
  z-index: -1;
}
.fk_center{
  width: 600rpx;
  height: 850rpx;
  /* border: 1px solid red; */
  z-index: 100;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.fk_title{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.fk_list{
  display: flex;
  justify-content: space-around;
  margin-top: 20rpx;
}
.fk_list view{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 110rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.contener_title{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-top: 20rpx;
}
.region_bg{
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.weui-input{
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #ccc;
  padding: 10rpx 0;
}